opam-version: "2.0"
name: "aoc"
version: "dev"
synopsis: "Advent of Code in Ocaml"
maintainer: "Drew Olson"
authors: "Drew Olson"
license: "MIT"
homepage: "https://github.com/drewolson/aoc-ocaml"
bug-reports: "https://github.com/drewolson/aoc-ocaml/issues"
depends: [
  "angstrom" {= "0.16.1"}
  "core" {= "v0.17.1"}
  "core_bench" {= "v0.17.0"}
  "core_kernel" {= "v0.17.0"}
  "core_unix" {= "v0.17.0"}
  "dune" {= "3.16.1"}
  "ocaml" {= "5.2.1"}
  "ocaml-lsp-server" {= "1.19.0"}
  "ocamlformat" {= "0.26.2"}
  "odig" {= "0.0.9"}
  "odoc" {= "2.4.3"}
  "pcre" {= "7.5.0"}
  "ppx_jane" {= "v0.17.0"}
  "utop" {= "2.15.0"}
  "z3" {= "4.13.3"}
  "zarith" {= "1.14"}
]
build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
dev-repo: "git+https://github.com/drewolson/aoc-ocaml.git"
